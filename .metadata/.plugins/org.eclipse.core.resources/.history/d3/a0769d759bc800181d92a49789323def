$(function() {
	
	$('#btn-salvar').click(function(event){
		
		var dados = $('#formCadastro').serializeArray();
		
		var dadosJson = {};

	    $.map(dados, function(n, i){
	    	dadosJson[n['name']] = n['value'];
	    });
		
	    console.log(dadosJason);
	   	
	    $.post('salvar', dadosJson);
		
		event.preventDefault();
		
	});
	
});